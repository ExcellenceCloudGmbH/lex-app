{

    "version": "0.2.0",
    "configurations": [
        {
            "name": "Django: Run Tests",
            "type": "python",
            "request": "launch",
            "program": "/vscode/DjangoProcessAdminGeneric/manage.py",
            "args": [
                "test"
            ],
            "django": true,
            "justMyCode": true,
            "cwd": "/vscode/DjangoProcessAdminGeneric/"
        },
        {
            "name": "Django: Run Server",
            "type": "python",
            "request": "launch",
            "program": "/vscode/DjangoProcessAdminGeneric/manage.py",
            "args": [
                "runserver",
                "0.0.0.0:8090"
            ],
            "django": true,
            "justMyCode": true,
            "cwd": "/vscode/DjangoProcessAdminGeneric/"
        }
    ]
}